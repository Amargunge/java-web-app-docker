pipeline{
    agentany
    stages{
        stage(SCM checkout){
            steps{
                git url: 'https://github.com/reddyvarisrinu95/javawebsrinu.git',branch: 'master'
            }
        stage(maven clean package){
            steps{
                sh "mvn clean package"
            }
        }    
        }
    }
}